@model IEnumerable<Shopping_Mall_Management.Models.RentOrLeaseRequest>
@Styles.Render("~/Content/Registration.css")
@{
    ViewBag.Title = "RequestStatus";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
<div class="Home">
    <h2>Request Status</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RequestId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StoreNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StoreLocation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.RequestId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.StoreNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.StoreLocation)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Type)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Status)
                </td>

            </tr>
        }

    </table>
</div>